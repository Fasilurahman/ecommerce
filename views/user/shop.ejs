                            <div class="products mb-3">
                                <div class="row justify-content-center">
                                    <% products.forEach(product => { %>
                                        <div class="col-6 col-md-4 col-lg-4">
                                            <div class="product product-7 text-center">
                                                <figure class="product-media">
                                                    <% if (product.isNew) { %>
                                                        <span class="product-label label-new">New</span>
                                                    <% } else if (product.isOutOfStock) { %>
                                                        <span class="product-label label-out">Out of Stock</span>
                                                    <% } %>
                                                    <a href="/product/<%= product._id %>">
                                                        <img src="/uploads/<%= product.images && product.images.length > 0 ? product.images[0] : '/assets/images/placeholder.png' %>" alt="Product image" class="product-image" style="height: 250px;">
                                                    </a>
                                                    <div class="product-action-vertical">
                                                        <a href="/wishlist" class="btn-product-icon btn-wishlist btn-expandable" onclick="addToWishlist('<%= product._id %>', event)">
                                                            <span>Add to Wishlist</span>
                                                        </a>
                                                    </div>
                                                    <div class="product-action" style="text-align: center;">
                                                        <% const selectedVariant = product.variants[0]; %> <!-- Assume default variant selection -->
                                                        <% const isSelectedVariantInStock = selectedVariant.stock > 0; %>
                                                        <% if (isSelectedVariantInStock) { %>
                                                            <button class="btn-product btn-cart" style="width: 100%;" onclick="addToCart('<%= product._id %>', '<%= selectedVariant._id %>')">
                                                                <span>Add to Cart</span>
                                                            </button>
                                                        <% } else { %>
                                                            <a href="javascript:void(0);" class="btn-product btn-cart" onclick="notifyOutOfStock()" style="width: 100%;">
                                                                <span>Out of Stock</span>
                                                            </a>
                                                        <% } %>
                                                    </div> 
                                                </figure>
                                                <div class="product-body">
                                                    <div class="product-cat">
                                                        <!-- <%= product.category && product.category.name ? product.category.name : 'Unknown Category' %> -->
                                                    </div>
                                                    <h3 class="product-title">
                                                        <a href="/product/<%= product._id %>"><%= product.name %></a>
                                                    </h3>
                                                    <div class="product-price">
                                                        <% if (product.offers && product.offers.length > 0) { %>
                                                            <% const offer = product.offers[0]; %>
                                                            <% const discount = typeof offer.discount === 'number' ? offer.discount : 0; %>
                                                            <% const originalPrice = typeof product.price === 'number' ? product.price : 0; %>
                            
                                                            <% if (discount > 0 && originalPrice > 0) { %>
                                                                <% const discountAmount = (originalPrice * discount) / 100; %>
                                                                <% const discountedPrice = originalPrice - discountAmount; %>
                            
                                                                <span class="old-price" style="text-decoration: line-through; color: #999;">
                                                                    ₹<%= originalPrice.toFixed(2) %>
                                                                </span>
                                                                <span class="new-price" style="color: #e63946; font-weight: bold; font-size: 1.2em;">
                                                                    ₹<%= discountedPrice.toFixed(2) %>
                                                                </span>
                                                            <% } else { %>
                                                                <span class="price" style="color: #333; font-size: 1.2em;">
                                                                    ₹<%= originalPrice.toFixed(2) %>
                                                                </span>
                                                            <% } %>
                                                        <% } else { %>
                                                            <span class="price" style="color: #333; font-size: 1.2em;">
                                                                ₹<%= product.price.toFixed(2) %>
                                                            </span>
                                                        <% } %>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    <% }) %>
                                </div>
                            </div>
                              <!-- SweetAlert Out of Stock Script -->
                              <nav aria-label="Page navigation">
                                <ul class="pagination justify-content-center">
                                    <li class="page-item <%= currentPage === 1 ? 'disabled' : '' %>">
