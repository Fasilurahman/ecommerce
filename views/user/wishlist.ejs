        .table-wishlist {
    width: 100%;
    margin-bottom: 1rem;
    background-color: #fff;
    border-radius: 0.5rem;
    overflow: hidden;
}

.table-wishlist th, .table-wishlist td {
    padding: 1rem;
    vertical-align: middle;
    border-top: 1px solid #dee2e6;
}

.product-col {
    display: flex;
    align-items: center;
}

.product-media img {
    width: 80px; /* Adjust as needed */
    height: auto;
    margin-right: 15px;
}

.product-title {
    margin: 0;
    font-size: 1rem; /* Adjust as needed */
}

.btn-remove {
    border: none;
    background: transparent;
    color: #dc3545;
    cursor: pointer;
}
.custom-variant-select {
  position: relative;
  width: 100%;
}

.variant-dropdown {
  border-radius: 0.5rem;
  padding: 0.6rem 1rem;
  background-color: #f8f9fa; /* Light grey */
  border: 1px solid #ced4da;
  font-weight: 500;
  color: #495057;
  transition: all 0.3s ease-in-out;
  width: 100%;
}

.variant-dropdown:focus {
  outline: none;
  border-color: #007bff; /* Primary color border on focus */
  box-shadow: 0 0 8px rgba(0, 123, 255, 0.25); /* Glowing effect */
}

.variant-dropdown:hover {
  background-color: #e9ecef; /* Slight hover effect */
}

.variant-dropdown option {
  padding: 0.5rem;
  font-weight: 400;
  color: #212529;
}

/* Adjust table cell to fit the dropdown neatly */
.variant-col {
  padding: 10px 12px;
  text-align: center;
}

@media (max-width: 768px) {
  .variant-col {
    padding: 8px 10px;
  }

  .variant-dropdown {
    font-size: 0.9rem; /* Adjust font size for smaller screens */
  }
}


