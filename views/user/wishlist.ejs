                                <tr>
                                    <th>Product</th>
                                    <th>Variant</th>
                                    <th>Price</th>
                                    <th>Stock Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% wishlist.products.forEach(wishlistItem => { %>
                                    <% const product = wishlistItem.productId; %>
