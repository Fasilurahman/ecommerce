                                        <td class="product-col">
                                            <div class="product">
                                                <figure class="product-media">
                                                    <a href="/product/<%= product._id %>">
                                                        <img src="/uploads/<%= product.images[0] %>" alt="Product image" class="img-fluid">
                                                    </a>
                                                </figure>
                                                <h3 class="product-title">
                                                    <a href="/product/<%= product._id %>">
                                                        <%= product.name %>
                                                    </a>
                                                </h3>
                                            </div>
                                        </td>
                                        
                                        <!-- Loop through product variants -->
                                        <% if (product.variants && product.variants.length > 0) { %>
                                            <td class="variant-col">
                                                <div class="custom-variant-select">
                                                    <select class="form-select variant-dropdown shadow-sm" id="variant-<%= product._id %>" onchange="updateVariantDetails('<%= product._id %>')">
                                                        <% product.variants.forEach(variant => { %>
                                                            <option value="<%= variant._id %>" data-price="<%= variant.price %>" data-stock="<%= variant.stock %>">
                                                                <%= variant.quantity %>
                                                            </option>
                                                        <% }) %>
                                                    </select>
                                                </div>
                                            </td>
                                            
                                            <td class="price-col" id="price-<%= product._id %>">
                                                $<%= product.variants[0].price.toFixed(2) %> <!-- Default to the first variant price -->
                                            </td>
            
                                            <td class="stock-status" id="stock-status-<%= product._id %>">
                                                <% if (product.variants[0].stock > 0) { %>
                                                    <span class="in-stock">In Stock</span>
                                                <% } else { %>
                                                    <span class="out-of-stock">Out of Stock</span>
                                                <% } %>
                                            </td>
                                        <% } else { %>
