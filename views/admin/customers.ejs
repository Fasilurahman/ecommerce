        const response = await fetch(
          `/admin/users?ajax=true&page=${page}&search=${encodeURIComponent(search)}`
        );
        const data = await response.json();

        const users = data.users;
        const totalPages = data.totalPages;
        currentPage = data.currentPage;
        currentSearch = search;

        const tableHtml = `
          <table class="table table-striped table-bordered" width="100%" cellspacing="0">
            <thead class="thead-dark">
              <tr>
                <th>Username</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Status</th>
                <th>Actions</th>
              </tr>
            </thead>
            <tbody>
              ${renderTableRows(users)}
            </tbody>
          </table>
        `;

        tableContainer.innerHTML = tableHtml;
        paginationContainer.innerHTML = renderPagination(totalPages, currentPage, currentSearch);
      } catch (error) {
        console.error("Error fetching users:", error);
      }
    }

    // Debounce search input
    let debounceTimer;
    searchInput.addEventListener("input", () => {
      clearTimeout(debounceTimer);
      debounceTimer = setTimeout(() => {
        fetchUsers(1, searchInput.value.trim());
      }, 300);
    });

    // Pagination click event (event delegation)
    paginationContainer.addEventListener("click", (e) => {
      e.preventDefault();
      if (e.target.tagName === "A") {
        const page = e.target.getAttribute("data-page");
        const search = e.target.getAttribute("data-search") || "";
        fetchUsers(parseInt(page), search);
      }
    });
  });
</script>


