    <script>
  async function toggleUserStatus(userId, shouldBlock, buttonEl) {
    console.log('toucing itskdlj')
    const action = shouldBlock ? 'block' : 'unblock';
    const confirmText = shouldBlock ? 'block this user' : 'unblock this user';

    const result = await Swal.fire({
      title: 'Are you sure?',
      text: `Do you want to ${confirmText}?`,
      icon: 'warning',
      showCancelButton: true,
      confirmButtonText: `Yes, ${action}`,
      cancelButtonText: 'Cancel'
    });

    if (!result.isConfirmed) return;

    try {
      const response = await fetch(`/admin/users/${action}/${userId}`, {
        method: 'PATCH',
        headers: {
          'Content-Type': 'application/json'
        }
      });

      if (response.ok) {
        // Update button appearance
        if (shouldBlock) {
          buttonEl.classList.remove('btn-warning');
          buttonEl.classList.add('btn-success');
          buttonEl.innerHTML = `<i class="fas fa-unlock-alt"></i> Unblock`;
          buttonEl.setAttribute('onclick', `toggleUserStatus('${userId}', false, this)`);
          buttonEl.closest('tr').children[3].textContent = 'Blocked';
        } else {
          buttonEl.classList.remove('btn-success');
          buttonEl.classList.add('btn-warning');
          buttonEl.innerHTML = `<i class="fas fa-lock"></i> Block`;
          buttonEl.setAttribute('onclick', `toggleUserStatus('${userId}', true, this)`);
          buttonEl.closest('tr').children[3].textContent = 'Active';
        }

        Swal.fire('Success', `User ${shouldBlock ? 'blocked' : 'unblocked'} successfully`, 'success');
      } else {
        Swal.fire('Error', 'Failed to update user status.', 'error');
      }
    } catch (err) {
      console.error(err);
      Swal.fire('Error', 'Something went wrong.', 'error');
    }
  }
</script>
<script>
  document.addEventListener("DOMContentLoaded", () => {
    const searchInput = document.getElementById("searchInput");
    const tableContainer = document.getElementById("tableContainer");
    const paginationContainer = document.getElementById("paginationContainer");

    let currentPage = Number("<%= typeof currentPage !== 'undefined' ? currentPage : 1 %>");
    let currentSearch = searchInput.value || "";

    function renderTableRows(users) {
      return users
        .map(
          (user) => `
        <tr>
          <td>${user.username}</td>
          <td>${user.email}</td>
          <td>${user.phone}</td>
          <td>${user.isBlocked ? "Blocked" : "Active"}</td>
          <td>
            ${
              user.isBlocked
                ? `<button class="btn btn-success btn-sm" onclick="toggleUserStatus('${user._id}', false, this)">
                    <i class="fas fa-unlock-alt"></i> Unblock
                   </button>`
                : `<button class="btn btn-warning btn-sm" onclick="toggleUserStatus('${user._id}', true, this)">
                    <i class="fas fa-lock"></i> Block
                   </button>`
            }
          </td>
        </tr>
      `
        )
        .join("");
    }

    function renderPagination(totalPages, currentPage, searchTerm) {
      if (totalPages <= 1) return "";

      let html = `<nav aria-label="Page navigation"><ul class="pagination">`;

      if (currentPage > 1) {
        html += `<li class="page-item">
            <a class="page-link" href="#" data-page="${currentPage - 1}" data-search="${searchTerm}">&laquo; Previous</a>
          </li>`;
      }

      for (let i = 1; i <= totalPages; i++) {
        html += `<li class="page-item ${currentPage === i ? "active" : ""}">
            <a class="page-link" href="#" data-page="${i}" data-search="${searchTerm}">${i}</a>
          </li>`;
      }

      if (currentPage < totalPages) {
        html += `<li class="page-item">
            <a class="page-link" href="#" data-page="${currentPage + 1}" data-search="${searchTerm}">Next &raquo;</a>
          </li>`;
      }

      html += `</ul></nav>`;
      return html;
    }

    async function fetchUsers(page = 1, search = "") {
